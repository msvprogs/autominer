{
  "ConnectionStrings": {
    "AutoMinerDb": "Server=MYSQL_SERVER_HOST;Database=MYSQL_DB_NAME;Uid=MYSQL_USER_NAME;Pwd=MYSQL_USER_PASSWORD;"
  },
  "RootCertificateFileName": "rootCa.cer", 
  "Services": {
    "CoinInfo": {
      "Url": "http://localhost:52045",
      "ApiKey": ""
    }
  },
  "Endpoints": {
    "Http": {
      "Enabled": false,
      "Port": 6285 
    },  
    "HttpsExternal": {
      "Enabled": true, 
      "Port": 6283,
      "Certificate": {
        "File": "HTTPS_CERTIFICATE.pfx",
        "Password": "HTTPS_CERT_PASSWORD" 
      } 
    },
    "HttpsInternal": {
      "Enabled": false, 
      "Port": 6284,
      "Certificate": {
        "File": "HTTPS_CERTIFICATE_INTERNAL.pfx",
        "Password": "HTTPS_CERT_INTERNAL_PASSWORD" 
      } 
    }
  },
  "NormalRigStateCriteria": {
    "SamplesCount": 5,
    "MinVideoUsage": 30,
    "MaxVideoTemperature": 75,
    "MaxInvalidSharesRate": 40,
    "MaxHashrateDifference": 30
  },
  "Notifications": {
    "Telegram": {
      "Enabled": false,
      "Token": "TELEGRAM_TOKEN",
      "Subscribers": [ "YOUR_TELEGRAM_USERNAME" ]
    }
  },
  "FileStorage": {
    "Miners": "C:\\UploadedMiners" 
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
