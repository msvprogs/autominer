<?xml version="1.0" encoding="utf-8" ?>
<Obfuscator>
  <Var name="InPath" value="." />
  <Var name="OutPath" value="$(InPath)\Obfuscator_Output" />
  <Var name="KeepPublicApi" value="false" />
  <Var name="HidePrivateApi" value="true" />
  <Var name="HideStrings" value="true" />

  <Module file="$(InPath)\Msv.AutoMiner.CoinInfoService.dll">
    <SkipNamespace name="Msv.AutoMiner.CoinInfoService.Configuration" />
    <SkipType name="Msv.AutoMiner.CoinInfoService.Startup" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
    <SkipType name="*AnonymousType*" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
    <SkipType name="*NLog*" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
  </Module>

  <Module file="$(InPath)\Msv.AutoMiner.Data.dll">
    <SkipNamespace name="Msv.AutoMiner.Data" />
    <SkipType name="*AnonymousType*" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
  </Module>

  <Module file="$(InPath)\Msv.AutoMiner.Common.dll">
    <SkipNamespace name="Msv.AutoMiner.Common.Configuration" />
    <SkipType name="Msv.AutoMiner.Common.Log.MemoryBufferTarget" skipProperties="true" />
    <SkipNamespace name="Msv.AutoMiner.Common.Models*" />
    <SkipType name="*AnonymousType*" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
  </Module>

  <Module file="$(InPath)\Msv.AutoMiner.NetworkInfo.dll">
    <!-- All methods that are called from dynamic context should be skipped :( -->
    <SkipMethod type="Msv.AutoMiner.NetworkInfo.Common.InsightInfoProvider" rx="GetDifficulty" />
    <SkipMethod type="Msv.AutoMiner.NetworkInfo.Common.IquidusInfoProvider" rx="GetDifficulty|GetRealHashRate" />
    <SkipMethod type="Msv.AutoMiner.NetworkInfo.Common.JsonRpcLocalNetworkInfoProvider" rx="ParseDifficulty" />
    <SkipMethod type="Msv.AutoMiner.NetworkInfo.Common.SwaggerInfoProviderBase" rx="GetBlockReward" />
    <SkipMethod type="Msv.AutoMiner.NetworkInfo.Common.TheCryptoChatInfoProvider" rx="GetBlockReward|GetApiResponse" />
    <SkipType name="*AnonymousType*" skipProperties="true" skipMethods="true" skipFields="true" skipEvents="true" />
  </Module>
  <Module file="$(InPath)\Msv.BrowserCheckBypassing.dll" />
  <Module file="$(InPath)\Msv.HttpTools.dll" />
</Obfuscator>